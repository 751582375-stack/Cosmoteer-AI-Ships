ID = aredja.ai_ships
Name = "AI Ships"
Version = "1.0.0"
CompatibleGameVersions = ["0.30.0"]
ModifiesGameplay = true
ModifiesMultiplayer = true
Author = "aredja"
Logo = "logo.png"
StringsFolder = "strings"
Description = "Enhances ships with AI cores that can operate nearby parts autonomously.\n\n"\
             "Features:\n"\
             "- AI cores that operate parts within a 2-tile radius\n"\
             "- Compatible with most vanilla parts that require crew\n"\
             "- Multiple AI cores can work together for parts requiring more crew\n"\
             "- Seamless integration with existing ship designs\n\n"\
             "Inspired by Aephix's AI Core mod. I've attempted to maintain compatibility with all the mods that Aephix's version supported. "\
             "If you find any compatibility issues, please let me know.\n\n"\
             "Currently includes support for vanilla parts and attempts compatibility with Modular Missiles mod."

Actions
[
	// -----------------------------------------------------
	// PART REGISTRATION: Add the AI cores to the game
	// -----------------------------------------------------
	
		// Add the small single-tile AI core to the Terran parts list
		{
			Action = Add
			AddTo = "<ships/terran/terran.rules>/Terran/Parts"
			ToAdd = &<single_core/single_core.rules>/Part
		}
		
		// Add the large 4x4 AI core to the Terran parts list
		{
			Action = Add
			AddTo = "<ships/terran/terran.rules>/Terran/Parts"
			ToAdd = &<large_core/large_core.rules>/Part
		}
		
		// Add the behemeth 7x7 AI core to the Terran parts list
		{
			Action = Add
			AddTo = "<ships/terran/terran.rules>/Terran/Parts"
			ToAdd = &<behemeth_core/behemeth_core.rules>/Part
		}
	
	// -----------------------------------------------------
	// TECHNOLOGY REGISTRATION: Add the AI cores to the tech tree
	// -----------------------------------------------------
	
	// Define the small AI core technology in the career mode tech tree
	// Requires Control Room Large and Sensor Array as prerequisites
	{
		Action = Add
		AddTo = "<modes/career/techs.rules>/Techs"
		ToAdd
		{
			ID = aredja.technology.ai_core
			NameKey = &<single_core/single_core.rules>/Part/NameKey
			DescriptionKey = &<single_core/single_core.rules>/Part/DescriptionKey
			Icon = &<single_core/single_core.rules>/Part/EditorIcon
			EditorGroups = ["Crew"]
			PartsUnlocked = [ &<single_core/single_core.rules>/Part/ID ]
			Cost = 15000
			Prerequisites = [ cosmoteer.control_room_large, cosmoteer.sensor_array ]
		}
	}
	
	// Define the large AI core technology in the career mode tech tree
	// Requires the small AI core as a prerequisite
	{
		Action = Add
		AddTo = "<modes/career/techs.rules>/Techs"
		ToAdd
		{
			ID = aredja.technology.ai_large_core
			NameKey = &<large_core/large_core.rules>/Part/NameKey
			DescriptionKey = &<large_core/large_core.rules>/Part/DescriptionKey
			Icon = &<large_core/large_core.rules>/Part/EditorIcon
			EditorGroups = ["Crew"]
			PartsUnlocked = [ &<large_core/large_core.rules>/Part/ID ]
			Cost = 300000
			Prerequisites = [ aredja.technology.ai_core ]
		}
	}
	
	// Define the behemeth AI core technology in the career mode tech tree
	// Requires the large AI core as a prerequisite
	{
		Action = Add
		AddTo = "<modes/career/techs.rules>/Techs"
		ToAdd
		{
			ID = aredja.technology.ai_behemeth_core
			NameKey = &<behemeth_core/behemeth_core.rules>/Part/NameKey
			DescriptionKey = &<behemeth_core/behemeth_core.rules>/Part/DescriptionKey
			Icon = &<behemeth_core/behemeth_core.rules>/Part/EditorIcon
			EditorGroups = ["Crew"]
			PartsUnlocked = [ &<behemeth_core/behemeth_core.rules>/Part/ID ]
			Cost = 1500000
			Prerequisites = [ aredja.technology.ai_large_core ]
		}
	}
	
	// Add custom stat display for AI Operating Power in the part stats panel
	{
		Action = Add
		AddTo = "<gui/game/parts/part_stats.rules>/PartStats"
		ToAdd
		{
			ID = Aredja2OperatingPower
			FormatKey = "Stats/Aredja2OperatingPower"
		}
	}
	
	// -----------------------------------------------------
	// BUFF DEFINITION: Define the AI Terminal buff
	// -----------------------------------------------------
	
	// Create or override the Aredja2AITerminal buff with visual styling
	{
		Action = Overrides
		OverrideIn = "<buffs/buffs.rules>/Aredja2AITerminal"
		CreateIfNotExisting = true
		Overrides
		{
			CombineMode = Add
			BaseValue = 0

			Icon
			{
				Texture
				{
					File = "single_core/buff_ai.png"
					MipLevels = 2
					SampleMode = Linear
				}
				Size = [2, 2]
			}
			IconTextFormatKey = "BuildBox/BuffAredja2AITerminal"

			RectBorderColor = [10, 212, 98, 160]
			RectFillColor = [10, 212, 98, 64]
		}
	}
	
	// Make all parts able to receive the AI Terminal buff
	{
		Action = Add
		AddTo = "<ships/base_part.rules>/Part/ReceivableBuffs"
		Name = Aredja2AITerminal
		ToAdd = Aredja2AITerminal
	}

